<span class="o">&gt;&gt;&gt;</span> <span class="n">name</span><span class="p">.</span><span class="n">rjust</span><span class="p">(</span><span class="mi">100</span><span class="p">,</span><span class="s">"*"</span><span class="p">)</span>
