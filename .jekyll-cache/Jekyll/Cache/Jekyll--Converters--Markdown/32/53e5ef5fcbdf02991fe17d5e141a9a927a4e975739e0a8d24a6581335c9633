    <span class="kd">let</span> <span class="nx">name</span> <span class="o">=</span> <span class="nx">val</span><span class="p">.</span><span class="nx">replace</span><span class="p">(</span><span class="sr">/</span><span class="se">(</span><span class="sr">.*</span><span class="se">\/)</span><span class="sr">*</span><span class="se">([^</span><span class="sr">.</span><span class="se">]</span><span class="sr">+</span><span class="se">)</span><span class="sr">.*/ig</span><span class="p">,</span><span class="dl">"</span><span class="s2">$2</span><span class="dl">"</span><span class="p">);</span>
