        <span class="kd">let</span> <span class="nx">output</span> <span class="o">=</span> <span class="nx">val</span><span class="p">.</span><span class="nx">replace</span><span class="p">(</span><span class="sr">/</span><span class="se">(</span><span class="sr">.*</span><span class="se">\\)(</span><span class="sr">.*</span><span class="se">\/)</span><span class="sr">*</span><span class="se">([^</span><span class="sr">.</span><span class="se">]</span><span class="sr">+</span><span class="se">)</span><span class="sr">.*/ig</span><span class="p">,</span><span class="dl">"</span><span class="s2">$2</span><span class="dl">"</span><span class="p">);</span>
        <span class="kd">let</span> <span class="nx">file</span> <span class="o">=</span> <span class="nx">val</span><span class="p">.</span><span class="nx">replace</span><span class="p">(</span><span class="sr">/</span><span class="se">(</span><span class="sr">.*</span><span class="se">\/)</span><span class="sr">*</span><span class="se">(</span><span class="sr">.+</span><span class="se">)</span><span class="sr">.*/ig</span><span class="p">,</span><span class="dl">"</span><span class="s2">$2</span><span class="dl">"</span><span class="p">);</span>
        <span class="kd">let</span> <span class="nx">htmlConfig</span> <span class="o">=</span> <span class="p">{</span>
			<span class="na">favicon</span><span class="p">:</span> <span class="nx">dirPath</span><span class="p">.</span><span class="nx">src</span><span class="o">+</span><span class="dl">'</span><span class="s1">/favicon.ico</span><span class="dl">'</span><span class="p">,</span>
			<span class="na">filename</span><span class="p">:</span> <span class="nx">output</span> <span class="o">+</span> <span class="nx">file</span><span class="p">,</span>   <span class="c1">//热替换时如果有[hash]则会找不到页面</span>
			<span class="na">template</span><span class="p">:</span> <span class="nx">val</span><span class="p">,</span>
			<span class="na">inject</span><span class="p">:</span><span class="dl">'</span><span class="s1">body</span><span class="dl">'</span><span class="p">,</span>		
			<span class="na">chunks</span><span class="p">:[</span><span class="s2">`runtimechunk~</span><span class="p">${</span><span class="nx">name</span><span class="p">}</span><span class="s2">`</span><span class="p">,</span><span class="dl">'</span><span class="s1">vendors</span><span class="dl">'</span><span class="p">,</span><span class="nx">name</span><span class="p">],</span>    <span class="c1">//只允许加载当前的chunk</span>
			<span class="na">chunksSortMode</span><span class="p">:</span><span class="dl">'</span><span class="s1">auto</span><span class="dl">'</span><span class="p">,</span>  <span class="c1">//加载chunk的顺序</span>
			<span class="na">excludeChunks</span><span class="p">:[],</span> <span class="c1">//排除某个 chunk</span>
        <span class="p">}</span>
        <span class="nx">plugins</span><span class="p">.</span><span class="nx">push</span><span class="p">(</span><span class="k">new</span> <span class="nx">HtmlWebpackPlugin</span><span class="p">(</span><span class="nx">htmlConfig</span><span class="p">));</span>
    <span class="p">}</span>
<span class="p">})</span>

<span class="nx">plugins</span><span class="p">.</span><span class="nx">unshift</span><span class="p">(</span><span class="k">new</span> <span class="nx">HtmlWebpackPlugin</span><span class="p">({</span>
	<span class="na">favicon</span><span class="p">:</span> <span class="nx">dirPath</span><span class="p">.</span><span class="nx">src</span><span class="o">+</span><span class="dl">'</span><span class="s1">/favicon.ico</span><span class="dl">'</span><span class="p">,</span>
	<span class="na">filename</span><span class="p">:</span><span class="dl">'</span><span class="s1">index.html</span><span class="dl">'</span><span class="p">,</span>   
	<span class="na">template</span><span class="p">:</span> <span class="nx">dirPath</span><span class="p">.</span><span class="nx">src</span> <span class="o">+</span> <span class="dl">'</span><span class="s1">/index.html</span><span class="dl">'</span><span class="p">,</span>
