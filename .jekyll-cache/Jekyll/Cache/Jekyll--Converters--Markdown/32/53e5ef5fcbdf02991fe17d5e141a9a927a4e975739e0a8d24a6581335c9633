	<span class="na">chunks</span><span class="p">:[</span><span class="dl">'</span><span class="s1">runtimechunk~main</span><span class="dl">'</span><span class="p">,</span><span class="dl">'</span><span class="s1">vendors</span><span class="dl">'</span><span class="p">,</span><span class="dl">'</span><span class="s1">vue</span><span class="dl">'</span><span class="p">,</span><span class="dl">'</span><span class="s1">main</span><span class="dl">'</span><span class="p">],</span>    <span class="c1">//只允许加载当前的chunk</span>
	<span class="na">chunksSortMode</span><span class="p">:</span><span class="dl">'</span><span class="s1">auto</span><span class="dl">'</span><span class="p">,</span>  <span class="c1">//加载chunk的顺序</span>
	<span class="na">excludeChunks</span><span class="p">:[]</span>     <span class="p">,</span> <span class="c1">//排除某个 chunk</span>
<span class="p">}))</span>

